# App Configuration
VITE_APP_NAME=Mint NFT

# Telegram Bot
VITE_TG_BOT_USERNAME=your_bot_username

# TON Network
VITE_TON_COLLECTION_ADDRESS=

# Mint Price (in nanoton)
# 1 TON = 1,000,000,000 nanoton
VITE_MINT_PRICE_NANOTON=1000000000

# Web3.Storage API Token
# Get from: https://web3.storage
VITE_WEB3STORAGE_TOKEN=YOUR_WEB3STORAGE_TOKEN

# Network (testnet or mainnet)
VITE_NETWORK=testnet

# Direct Mint API Configuration
# Địa chỉ ví nhận tiền mint (API wallet)
VITE_API_WALLET_ADDRESS=

# Backend API endpoint (server nhận thông tin mint)
VITE_API_ENDPOINT=https://api.yourdomain.com
